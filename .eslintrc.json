{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:vue/essential",
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "User": true,
        "EventBus": true,
        "AppStorage": true,
        "axios": true,
        "md": true,
        "Echo": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "parser": "@typescript-eslint/parser",
        "sourceType": "module"
    },
    "plugins": [
        "vue",
        "@typescript-eslint"
    ],
    "rules": {
        "no-class-assign": 0,
        "class-methods-use-this": 0,
        "camelcase": 0,
        "import/extensions": 0,
        "import/no-extraneous-dependencies": 0,
        "import/first": 0,
        "no-console": 0,
        "no-unused-vars": 0,
        "no-unused-expressions": 0,
        "no-plusplus": 0
    }
}